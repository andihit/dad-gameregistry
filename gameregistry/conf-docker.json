// Configuration json file used in the docker image (production). See Readme.md.
// This configures mongo-persistor to connect to a mongo server named 'mongo-server'.
{
  "mongo-persistor" : {
    "address" : "gameregistry.db",
    "host" : "mongo-server",
    "db_name" : "gameregistry"
  },

  "game-registry" : {
    "static_web_basepath" : "/",
    "host" : "0.0.0.0",
    "port" : 8080,
    "debug_promise" : false
  }
}
